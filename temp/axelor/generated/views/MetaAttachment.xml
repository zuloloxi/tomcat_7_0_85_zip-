  <grid edit-icon="true" name="meta-attachment-grid" title="Meta attachments" model="com.axelor.meta.db.MetaAttachment">
    <field name="objectName"/>
    <field name="objectId"/>
    <field name="metaFile"/>
    <field name="archived"/>
